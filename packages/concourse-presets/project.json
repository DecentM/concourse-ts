{
  "name": "concourse-presets",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/concourse-presets/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/concourse-presets",
        "main": "packages/concourse-presets/src/index.ts",
        "tsConfig": "packages/concourse-presets/tsconfig.lib.json",
        "assets": ["packages/concourse-presets/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/concourse-presets/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "{workspaceRoot}/coverage/{projectRoot}"
      }
    }
  },
  "tags": []
}
