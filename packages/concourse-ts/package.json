{
  "name": "@decentm/concourse-ts",
  "version": "0.9.3",
  "homepage": "https://github.com/DecentM/concourse-ts/tree/main/packages/concourse-ts",
  "repository": "https://github.com/DecentM/concourse-ts.git",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "nx build",
    "dev": "ts-node-dev playground.ts && fly vp -c playground.yml",
    "dev:cli": "rimraf ci-import && ts-node-dev src/decompiler/cli-test.ts import -i src/decompiler/test/pipeline.yml -o ci-import",
    "t": "yarn test",
    "test": "c8 --reporter=lcov --reporter=text ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=ts-node/esm/transpile-only"
    ]
  },
  "resolutions": {
    "react": "18.2.0"
  },
  "dependencies": {
    "@swc/helpers": "~0.4.11",
    "fast-glob": "^3.2.12",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "ink-table": "^3.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "mkdirp": "^2.1.3",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "verror": "^1.10.1",
    "yaml": "^2.2.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/prettier": "^2.7.2",
    "@types/verror": "^1.10.6",
    "@types/yargs": "^17.0.17",
    "ava": "^5.2.0",
    "c8": "^7.12.0",
    "cpy-cli": "^4.2.0",
    "husky": "^8.0.2",
    "rimraf": "^4.1.2",
    "ts-node-dev": "^2.0.0"
  }
}