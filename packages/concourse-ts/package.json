{
  "name": "@decentm/concourse-ts",
  "version": "0.22.1",
  "homepage": "https://github.com/DecentM/concourse-ts/tree/main/packages/concourse-ts",
  "repository": "https://github.com/DecentM/concourse-ts.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ],
    "environmentVariables": {
      "TSIMP_DIAG": "ignore"
    },
    "timeout": "15s",
    "concurrency": 2
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "csstree-validator": "^4.0.1",
    "lodash.clonedeep": "^4.5.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "verror": "^1.10.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^24.9.1",
    "@types/prettier": "^3.0.0",
    "@types/verror": "^1.10.11",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "cpy-cli": "^6.0.0",
    "pkgroll": "^2.20.1",
    "rimraf": "^6.0.1",
    "tmp-promise": "^3.0.3",
    "tsimp": "^2.0.12"
  }
}